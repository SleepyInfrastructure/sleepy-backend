{
	"id": "cron",
	"name": "cron",
	"type": "CRON",
	"intervals": [
		{
			"time": "*/1 * * * *",
			"updates": [{ "type": "STATISTICS", "statistic": "MINUTE", "resources": ["GENERAL", "DISKS"] }],
			"cleans": [{ "type": "STATISTICS", "statistic": "MINUTE", "resources": ["GENERAL", "DISKS"] }]
		},
		{
			"time": "0 */1 * * *",
			"updates": [{ "type": "STATISTICS", "statistic": "HOUR", "resources": ["GENERAL", "DISKS"] }],
			"cleans": [{ "type": "STATISTICS", "statistic": "HOUR", "resources": ["GENERAL", "DISKS"] }]
		},
		{
			"time": "0 0 */1 * *",
			"updates": [{ "type": "STATISTICS", "statistic": "DAY", "resources": ["GENERAL", "DISKS"] }],
			"cleans": [{ "type": "STATISTICS", "statistic": "DAY", "resources": ["GENERAL", "DISKS"] }]
		},
		{
			"time": "0 0 1 */1 *",
			"updates": [{ "type": "STATISTICS", "statistic": "MONTH", "resources": ["GENERAL", "DISKS"] }],
			"cleans": [{ "type": "STATISTICS", "statistic": "MONTH", "resources": ["GENERAL", "DISKS"] }]
		},
		{
			"time": "0 0 1 1 */1",
			"updates": [{ "type": "STATISTICS", "statistic": "YEAR", "resources": ["GENERAL", "DISKS"] }],
			"cleans": [{ "type": "STATISTICS", "statistic": "YEAR", "resources": ["GENERAL", "DISKS"] }]
		},
		{
			"time": "*/1 * * * *",
			"updates": [{ "type": "RESOURCES", "resources": ["CONTAINERS"] }],
			"cleans": []
		},
		{
			"time": "*/5 * * * *",
			"updates": [{ "type": "RESOURCES", "resources": ["GENERAL", "DISKS"] }],
			"cleans": [{ "type": "TASKS", "time": 2628000 }]
		},
		{
			"time": "*/1 * * * *",
			"updates": [{ "type": "UPTIME_ENDPOINTS" }],
			"cleans": [{ "type": "UPTIME_STATISTICS", "time": 600 }]
		}
	]
}