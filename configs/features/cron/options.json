{
	"id": "cron",
	"name": "cron",
	"type": "CRON",
	"intervals": [
		{
			"time": "*/1 * * * *",
			"updates": [{ "type": "STATISTICS", "statistic": "MINUTE" }],
			"cleans": [{ "type": "STATISTICS", "statistic": "MINUTE" }]
		},
		{
			"time": "* */1 * * *",
			"updates": [{ "type": "STATISTICS", "statistic": "HOUR" }],
			"cleans": [{ "type": "STATISTICS", "statistic": "HOUR" }]
		},
		{
			"time": "* * */1 * *",
			"updates": [{ "type": "STATISTICS", "statistic": "DAY" }],
			"cleans": [{ "type": "STATISTICS", "statistic": "DAY" }]
		},
		{
			"time": "* * * */1 *",
			"updates": [{ "type": "STATISTICS", "statistic": "MONTH" }],
			"cleans": [{ "type": "STATISTICS", "statistic": "MONTH" }]
		},
		{
			"time": "* * * * */1",
			"updates": [{ "type": "STATISTICS", "statistic": "YEAR" }],
			"cleans": [{ "type": "STATISTICS", "statistic": "YEAR" }]
		},
		{
			"time": "*/1 * * * *",
			"updates": [{ "type": "RESOURCES", "resources": ["CONTAINERS"] }],
			"cleans": []
		},
		{
			"time": "*/5 * * * *",
			"updates": [{ "type": "RESOURCES", "resources": ["GENERAL", "DISKS"] }],
			"cleans": [{ "type": "TASKS", "time": 2628000 }]
		},
		{
			"time": "*/1 * * * *",
			"updates": [{ "type": "UPTIME_ENDPOINTS" }],
			"cleans": [{ "type": "UPTIME_STATISTICS", "time": 600 }]
		}
	]
}