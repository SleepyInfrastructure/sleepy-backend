{
    "name": "sleepy-backend",
    "version": "0.0.0",
    "description": "A customizable and modular web back-end.",
    "license": "GPL-3.0-or-later",
    "author": {
        "name": "LamkasDev",
        "url": "https://lamkas.dev"
    },
    "scripts": {
        "dev": "tsc && node build/index.js --dev",
        "build": "tsc",
        "serve": "node build/index.js",
        "lint": "eslint src/**/*.{ts,tsx}",
        "linty": "eslint src/**/*.{ts,tsx} --fix",
        "pretty": "prettier --write 'src/**/*.{ts,tsx}'",
        "typecheck": "tsc --noEmit",
        "finstall:static": "yarn add fastify@^4.5.2 @fastify/cors@^8.1.0 @fastify/static@^6.5.0",
        "finstall:api": "yarn add fastify@^4.5.2 @fastify/cors@^8.1.0 @fastify/cookie@^8.0.0 @fastify/rate-limit@^7.3.0 bcrypt@^5.0.1 && yarn add -D @fastify/type-provider-typebox@^2.3.0 @types/bcrypt@^5.0.0",
        "finstall:push": "yarn add web-push@^3.4.5 && yarn add -D @types/web-push@^3.3.2",
        "finstall:mysql": "yarn add mysql2@^2.3.3",
        "finstall:redis": "yarn add redis@^3.1.2 && yarn add -D @types/redis@^2.8.32",
        "finstall:ws": "yarn add @fastify/websocket@^7.0.0 bufferutil@^4.0.6 utf-8-validate@^5.0.9 && yarn add -D @types/ws@^8.5.3",
        "finstall:cron": "yarn add node-cron@^3.0.2 && yarn add -D @types/node-cron@^3.0.2",
        "funinstall": "yarn remove bcrypt mysql2 redis fastify @fastify/cors @fastify/cookie @fastify/static @fastify/websocket bufferutil utf-8-validate node-cron @fastify/type-provider-typebox @types/bcrypt @types/web-push @types/ws @types/redis"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@fastify/type-provider-typebox": "^2.3.0",
        "@types/archiver": "^5.3.1",
        "@types/bcrypt": "^5.0.0",
        "@types/node": "^18.7.13",
        "@types/node-cron": "^3.0.2",
        "@types/web-push": "^3.3.2",
        "@types/ws": "^8.5.3",
        "@typescript-eslint/eslint-plugin": "^5.35.1",
        "@typescript-eslint/parser": "^5.35.1",
        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.1",
        "prettier": "2.7.1",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@fastify/cookie": "^8.0.0",
        "@fastify/cors": "^8.1.0",
        "@fastify/multipart": "^7.1.0",
        "@fastify/rate-limit": "^7.3.0",
        "@fastify/static": "^6.5.0",
        "@fastify/websocket": "^7.0.0",
        "ajv": "^8.11.0",
        "archiver": "^5.3.1",
        "axios": "^0.27.2",
        "bcrypt": "^5.0.1",
        "bufferutil": "^4.0.6",
        "fastify": "^4.5.2",
        "mysql2": "^2.3.3",
        "nanocolors": "^0.2.12",
        "node-cron": "^3.0.2",
        "node-ssh": "^13.0.0",
        "pingman": "^1.1.5",
        "utf-8-validate": "^5.0.9",
        "web-push": "^3.5.0",
        "zod": "^3.19.1"
    }
}
